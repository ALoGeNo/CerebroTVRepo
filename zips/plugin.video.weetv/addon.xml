<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.weetv" name="WeeTV" version="4.1.9" provider-name="@geeteetv">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
    <import addon="script.module.weescrapers"/>
    <import addon="script.module.urlresolver" optional="true"/>
	<import addon="script.module.requests" />
	<import addon="script.module.beautifulsoup4" />
	<import addon="script.module.simplejson" version="3.3.0"/>
    <import addon="script.module.liveresolver" version="0.1.43"/>														   
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>
	<import addon="script.video.F4mProxy" />
    <import addon="plugin.video.f4mTester" />																 
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">WeeTV</summary>
    <description lang="en">  Canadian, US, UK Channels, TV shows and Movies.</description>
	<disclaimer lang="en">The original author neither hosts or stores any content, has no affiliation with content hosts. It is the sole responsibility of the user to ensure they have permission to view any streams provided</disclaimer>
	<platform>all</platform>
  </extension>
</addon>
