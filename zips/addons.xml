<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.cerebro" name=".CerebroTVRepo" version="2.1" provider-name="Biglad">
	<extension point="xbmc.addon.repository" name=".CrebroTVRepo">
	<dir>
		<info compressed="false">https://raw.githubusercontent.com/biglad/CerebroTVRepo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/biglad/CerebroTVRepo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/biglad/CerebroTVRepo/master/zips/</datadir>
	</dir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary></summary>
		<description>Cerebro TV Repo</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.cerebrotv"
    name=".[COLOR=red]Cerebro BUILD INSTALLER (PREMIUM)[/COLOR] NEED A PIN CODE TO INSTALL!!!" version="1.0.4" provider-name="CerebroTV">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script" library="addon.py">
     <provides>executable video</provides>
  </extension>
  <extension point="xbmc.service" library="addon.py" start="startup" />
  <extension point="xbmc.addon.metadata">
<language></language>
    <platform>all</platform>
    <license>CEREBROTV 2017</license>
    <forum>CEREBROTV 2017</forum>
    <source></source>
  </extension>
</addon>

<addon id="script.module.showboxarize2" name="Cerebro ShowBox Scraper" version="1.00.350" provider-name="biglad">
	<requires>
		<import addon="xbmc.python" version="2.19.0"/>
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.addon.metadata">
		<summary>Scrapers For Cerebro ShowBox Addon</summary>
		<description>...</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
